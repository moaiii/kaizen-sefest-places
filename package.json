{
  "name": "kaizen-safest-places",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^3.5.1",
    "flow-bin": "^0.72.0",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/stylesheet/ -o src/stylesheet/",
    "watch-css": "npm run build-css && node-sass-chokidar src/stylesheet/ -o src/stylesheet/ --watch",
    "start": "concurrently \"npm run watch-css\" \"react-scripts start\"",
    "dev": "concurrently \"react-scripts start\" \"npm run watch-css\" \"npm run flow\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
  "devDependencies": {
    "flow-watch": "^1.1.3"
  }
}
